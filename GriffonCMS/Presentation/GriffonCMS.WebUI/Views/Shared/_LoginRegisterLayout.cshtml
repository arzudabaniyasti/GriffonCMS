
@{
    Layout = null;
}

<!DOCTYPE html>
<title>Title</title>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>iofrm</title>
    <link rel="stylesheet" type="text/css" href="~/admin_assets/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/admin_assets/css/fontawesome-all.min.css">
    <link rel="stylesheet" type="text/css" href="~/admin_assets/css/iofrm-style.css">
    <link rel="stylesheet" type="text/css" href="~/admin_assets/css/iofrm-theme8.css">
</head>
<body>
    <div class="form-body" class="container-fluid">
        <div class="website-logo">
            <a href="index.html">
                <div class="logo">
                    <img class="logo-size" src="~/admin_assets/images/logo-light.svg" alt="">
                </div>
            </a>
        </div>
        <div class="row">
            <div class="img-holder">
                <div class="bg"></div>
                <div class="info-holder">
                    <h3>Get more things done with Loggin platform.</h3>
                    <p>Access to the most powerfull tool in the entire design and web industry.</p>
                    <img src="~/admin_assets/images/graphic4.svg" alt="">
                </div>
            </div>
            <div class="form-holder">
                <div class="form-content">
                    <div class="form-items">
                        <div class="website-logo-inside">
                            <a href="index.html">
                                <div class="logo">
                                    <img class="logo-size" src="~/admin_assets/images/logo-light.svg" alt="">
                                </div>
                            </a>
                        </div>
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="~/admin_assets/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/admin_assets/js/popper.min.js"></script>
    <script type="text/javascript" src="~/admin_assets/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/admin_assets/js/main.js"></script>
</body>
</html>

